            android:pathData="M 100 350 l 150 -300"
            />

        <path
            android:name="lineAD"
            android:strokeColor="#ff0000"
            android:strokeWidth="3"
            android:pathData="M 100 350 l 150 300"
            />
        <path
            android:name="lineBC"
            android:strokeColor="#ff0000"
            android:strokeWidth="3"
            android:pathData="M 250 50 l 150 300"
            />
        <path android:pathData="M 175 200 l 150 0"
            android:strokeWidth="3"
            android:strokeColor="@color/green"/>

        <path android:pathData="M 100 350 q 150 -300 300 0"
            android:strokeWidth="3"
            android:strokeColor="@color/blue"/>

        <g android:strokeColor="@color/black"
            android:strokeWidth="3">

        </g>

    </group>
</vector>